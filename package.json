{
  "name": "osc-msg",
  "description": "isomorphic OSC (Open Sound Control) message encoder/decoder",
  "version": "0.0.1",
  "author": "nao yonamine <mohayonao@gmail.com>",
  "bugs": {
    "url": "https://github.com/mohayonao/osc-msg/issues"
  },
  "dependencies": {
    "dataview2": "^0.1.0"
  },
  "devDependencies": {
    "babel": "^5.2.6",
    "babel-eslint": "^3.0.1",
    "babelify": "^6.0.2",
    "browserify": "^10.0.0",
    "eslint": "^0.20.0",
    "espower-babel": "^2.1.1",
    "isparta": "^3.0.3",
    "mocha": "^2.2.4",
    "osc-min": "^0.2.0",
    "power-assert": "^0.11.0",
    "uglify-js": "^2.4.20"
  },
  "files": [
    "package.json",
    "lib",
    "README.md"
  ],
  "homepage": "http://mohayonao.github.io/osc-msg/",
  "keywords": [
    "osc"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/mohayonao/osc-msg.git"
  },
  "scripts": {
    "build": "npm run build-to5 && npm run build-browser && npm run build-minify",
    "build-browser": "browserify src -t babelify --outfile build/osc-msg.js",
    "build-minify": "uglifyjs build/osc-msg.js -o build/osc-msg.min.js",
    "build-to5": "babel src --out-dir lib",
    "cover": "babel-node $(npm bin)/isparta cover --report text --report html --report lcov $(npm bin)/_mocha",
    "lint": "eslint src test",
    "mocha": "mocha",
    "prepublish": "npm run lint && npm run mocha && npm run build",
    "test": "mocha --require espower-babel/guess",
    "travis": "npm run lint && npm run cover"
  }
}
