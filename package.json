    "eslint": "^1.10.3",
    "eslint-config-mohayonao": "^0.1.0",
    "espower-babel": "^3.3.0",
    "isparta": "^3.5.3",
    "lodash.flatten": "^3.0.2",
    "mocha": "^2.3.4",
    "osc-min": "^0.2.0",
